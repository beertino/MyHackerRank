    "def swap_char(c):\n",
    "    if c.isupper():\n",
    "        return c.lower()\n",
    "    else:\n",
    "        return c.upper()\n",
    "\n",
    "def swap_case(s):\n",
    "    s=list(s)\n",
    "    return ''.join(list(map(swap_char,s)))\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    s = input()\n",
    "    result = swap_case(s)\n",
    "    print(result)"
   ]
